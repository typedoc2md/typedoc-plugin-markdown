{
  "entryPoints": ["./stubs/module-toc.ts"],
  "tsconfig": "./stubs/tsconfig.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-remark"],
  "remarkPlugins": [["remark-toc", { "maxDepth": 3 }]],
  "readme": "none",
  "disableSources": true,
  "outputs": [
